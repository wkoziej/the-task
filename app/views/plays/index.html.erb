<h1>Listing plays</h1>

<table>
  <tr>
    <th>Statedat</th>
    <th>Finishedat</th>
    <th>Status</th>
    <th>Game</th>
    <th>Player</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @plays.each do |play| %>
  <tr>
    <td><%= play.startedAt %></td>
    <td><%= play.finishedAt %></td>
    <td><%= play.status %></td>
    <td><%= play.game.title %></td>
    <td><%= play.player.login %></td>
    <td><%= link_to 'Show', play %></td>
 </tr>
<% end %>
</table>

<br />

